        </div>

        <div class="col-sm-4 feature-images-col">
          <%= image_tag('chub.jpg', :class => 'welcome_images img-circle ') %>
          <h3>Tag your friends!</h3>
        </div>
        <div class="col-sm-4 feature-images-col">
          <%= image_tag('puma.jpg', :class => 'welcome_images img-thumbnail') %>
