<h1>My Photos</h1>

<% Photo.all.each do |photo| %>
  <% if photo.user_id == @user.id %>

  <div class="grey">
    <p><%=link_to image_tag(photo.image.url(:thumb)), new_photo_tag_path(photo) %></p>



  </div>
  <% end %>
<% end %>




<p><%= link_to "Add a new photo", new_user_photo_path(current_user) %>

